{
    "name": "silverstripe/theme-fontpicker",
    "description": "Customise site fonts from the admin settings",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.1",
        "silverstripe/fontpicker": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\Fontpicker\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
